{
  "name": "reacterminator",
  "engines": {
    "node": ">=6.1.0"
  },
  "description": "Convert html files to react components",
  "main": "lib/index.js",
  "bin": "bin/reacterminator.js",
  "scripts": {
    "test": "standard && istanbul cover _mocha && coveralls < coverage/lcov.info",
    "watch": "rm -rf ./reacterminator && mocha --watch",
    "cover": "standard && istanbul cover _mocha",
    "doc": "jsdoc lib -d doc",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/poetic/reacterminator"
  },
  "keywords": [
    "react",
    "html",
    "converter",
    "generator",
    "html to react"
  ],
  "author": "Chun-Yang <yang2007chun@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/poetic/reacterminator/issues"
  },
  "homepage": "https://github.com/poetic/reacterminator",
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "istanbul": "^0.4.4",
    "jsdoc": "^3.4.0",
    "mocha": "^2.5.3",
    "standard": "^7.1.2",
    "semantic-release": "^4.3.5"
  },
  "standard": {
    "ignore": [
      "/coverage",
      "/doc",
      "/lib/plugins/redux/process-all-formatted-snippets/templates"
    ]
  },
  "dependencies": {
    "babel-generator": "^6.11.0",
    "babylon": "^6.8.2",
    "chalk": "^1.1.3",
    "cheerio": "^0.20.0",
    "commander": "^2.9.0",
    "esformatter": "^0.9.5",
    "esformatter-eol-last": "^1.0.0",
    "esformatter-jsx": "^7.0.1",
    "esformatter-quotes": "^1.1.0",
    "esformatter-semicolons": "^1.1.2",
    "glob": "^7.0.5",
    "lodash": "^4.13.1",
    "shelljs": "^0.7.0"
  }
}
